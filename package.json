{
  "name": "node-ams",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "migrate": "sequelize-cli db:migrate",
    "migrate-rollback": "sequelize-cli db:migrate:undo",
    "migrate-reset": "sequelize-cli db:migrate:undo:all",
    "migrate-fresh": "npm run migrate-reset && npm run migrate",
    "db-seed": "sequelize-cli db:seed:all",
    "db-seed-fresh": "npm run migrate-fresh && npm run db-seed"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "connect-session-sequelize": "^7.1.3",
    "csurf": "^1.11.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "joi": "^17.6.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.3",
    "sequelize": "^6.19.0",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@ngneat/falso": "^4.0.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  }
}
